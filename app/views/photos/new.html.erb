<div class="container">
  <div class="row">
    <div class="col-md-8 col-md-offset-2">
      <h1>Add a new photo</h1>

      <%= simple_form_for [@drone, @photo] do |f| %>
        <%= f.input :photo %>
        <%= f.button :submit, class: 'btn btn-primary' %>
      <% end %>
    </div>
  </div>
</div>
